{
  // Call hierarchy
  "callHierarchy.enableTooltip": true,
  "callHierarchy.openCommand": "vsplit",
  "callHierarchy.splitCommand": "botright 30vs",
  
  // CodeLens
  "codeLens.enable": true,
  "codeLens.position": "right_align",
  "codeLens.separator": "‣",
  "codeLens.subseparator": "/",

  // Colors highlight
  "colors.enable": false,
  "colors.highlightPriority": 1000,

  // Cursors
  "cursors.cancelKey": "<esc>",
  "cursors.nextKey": "<c-j>",
  "cursors.previousKey": "<c-k>",
  "cursors.wrapscan": true,

  // Diagnostic
  "diagnostic.autoRefresh": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enable": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.filetypeMap": {},
  "diagnostic.floatConfig": {
    "border": true,
    // "rounded": true,
    // "highlight": "CocFloating",
    // "borderhighlight": "CocFloating",
    // "close": false,
    // "maxWidth": 160,
    // "maxHeight": 10,
    // "winblend": 30,
    // "focusable": false,
    // "shadow": false,
  },
  "diagnostic.format": "[%source%code][%severity] %message",
  "diagnostic.highlightLimit": 1000,
  "diagnostic.highlightPriority": 4096,
  "diagnostic.level": "hint",
  "diagnostic.locationlistLevel": "hint",
  "diagnostic.locationlistUpdate": true,
  "diagnostic.messageDelay": 150,
  "diagnostic.messageLevel": "hint",
  "diagnostic.messageTarget":"float",
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.separateRelatedInformationAsDiagnostics": false,
  "diagnostic.showDeprecated": true,
  "diagnostic.showUnused": true,
  "diagnostic.signLevel": "hint",
  "diagnostic.signPriority": 0,
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextAlign": "after",
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.virtualTextFormat": "[%source%code][%severity] %message",
  "diagnostic.virtualTextLevel": "error",
  "diagnostic.virtualTextLimitInOneLine": 999,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextLines": 3,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualTextWinCol": 160,

  // Dialog
  "dialog.confirmKey": "<cr>",
  "dialog.floatBorderHighlight": "CocFloating",
  "dialog.floatHighlight": "CocFloating",
  "dialog.maxHeight": 30,
  "dialog.maxWidth": 160,
  "dialog.pickerButtonShortcut": true,
  "dialog.pickerButtons": true,
  "dialog.rounded": true,
  "dialog.shortcutHighlight": "MoreMsg",

  // DocumentHighlight
  "documentHighlight.priority": -1,
  "documentHighlight.timeout": 500,

  // FloatFactory
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": false,
    "highlight": "CocFloating",
    "borderhighlight": "CocFloating",
    "close": false,
    "maxWidth": 160,
    // "maxHeight": 10,
    "winblend": 0,
    "focusable": false,
    "shadow": false,
  },

  // Hover
  "hover.autoHide": true,
  "hover.floatConfig": {},
  "hover.previewMaxHeight": 12,
  "hover.target": "float",

  // Http Proxy
  // "http.proxy": "",
  // "http.proxyAuthorization": null,
  // "http.proxyCA": null,
  // "http.proxyStrictSSL": false,

  // InlayHint
  "inlayHint.enable": true,
  "inlayHint.enableParameter": true,
  "inlayHint.display": true,
  "inlayHint.parameterSeparator": "",
  "inlayHint.refreshOnInsertMode": true,
  "inlayHint.subSeparator": " ",
  "inlayHint.typeSeparator": "",

  // Completion
  "suggest.autoTrigger": "always",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.asciiCharactersOnly": false,
  "suggest.asciiMatch": true,
  "suggest.completionItemKindLabels": {
    "function": "󰊕",
    "variable": "",
    "constant": "",
    "constructor": "",
    "method": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "typeParameter": "",
    "field": "",
    "class": "",
    "struct": "",
    "keyword": "",
    "value": "",
    "operator": "",
    "reference": "",
    "interface": "",
    "text": "",
    "color": "",
    "property": "",
    "unit": "",
    "event": "",
    "file": "",
    "folder": "",
    "snippet": "ﴬ",
    "default": ""
  },

  "suggest.noselect": false,
  "suggest.selection": "first",
  "suggest.formatItems": [
    "abbr",
    "menu",
    "kind",
    "shortcut"
  ],
  "suggest.enablePreselect": true,
  "suggest.floatConfig": {},
  "suggest.labelMaxLength": 200,
  "suggest.detailField": "preview",
  "suggest.languageSourcePriority": 99,
  "suggest.snippetIndicator": " ",
  "suggest.maxCompleteItemCount": 50,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.snippetsSupport": true,
  "suggest.localityBonus": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 5000,
  "suggest.minTriggerInputLength": 1,
  "suggest.triggerCompletionWait": 0,
  "suggest.lowPrioritySourceLimit": 90,
  "suggest.removeDuplicateItems": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.invalidInsertCharacters": [
    " ",
    "(",
    "<",
    "{",
    "[",
    "\r",
    "\n"
  ],
  "suggest.ignoreRegexps": [],
  "suggest.virtualText": true,

  // Links
  "links.tooltip": true,


  // Signature
  "signature.enable": true,
  "signature.floatConfig": {},
  "signature.triggerSignatureWait": 500,
  "signature.target":"float",
  "signature.preferShownAbove": true,
  "signature.hideOnTextChange": false,

  // Refactor buffer
  "refactor.saveToFile": true,
  "refactor.openCommand": "vsplit",
  "refactor.beforeContext": 3,
  "refactor.afterContext": 3,
  "refactor.showMenu": "<Tab>",


  // Notification
  "notification.maxWidth": 120,
  "notification.maxHeight": 10,
  "notification.minProgressWidth": 30,
  "notification.highlightGroup": "CocFloating",
  "notification.winblend": 30,
  "notification.border": true,
  "notification.timeout": 10000,
  "notification.marginRight": 10,
  "notification.focusable": true,

  //Workspace related
  "workspace.openOutputCommand": "vs",
  "workspace.openResourceCommand": "tab drop",
  "workspace.ignoredFiletypes": [],
  "workspace.ignoredFolders": [
    "$HOME"
  ],
  "workspace.bottomUpFiletypes": [],
  "workspace.workspaceFolderCheckCwd": true,
  "workspace.workspaceFolderFallbackCwd": true,

  // List
  "list.indicator": ">",
  "list.alignColumns": true,
  "list.menuAction": false,
  "list.height": 20,
  "list.signOffset": 900,
  "list.selectedSignText": "",
  "list.maxPreviewHeight": 12,
  "list.previewHighlightGroup": "Search",
  "list.previewToplineStyle": "offset",
  "list.previewToplineOffset": 3,
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "list.extendedSearchMode": true,
  "list.normalMappings": {},
  "list.insertMappings": {},
  "list.interactiveDebounceTime": 100,
  "list.previewSplitRight": false,
  "list.source.gstatus.defaultAction": "add",

  // Preferences
  "coc.preferences.enableLinkedEditing": false,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.maxFileSize": "10MB",
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.extensionUpdateCheck": "weekly",
  // "coc.preferences.snippetHighlight": false,
  // "coc.preferences.snippetStatusText": "",

  /* "coc.preferences.hoverTarget": "float", */
  "coc.preferences.jumpCommand": "tab drop",
  "workspace.rootPatterns": [
    "package.json", "*.pom", "requirements.txt", "Rakefile", "Makefile", "Cargo.toml", "go.mod", ".git"
  ],

  "npm.binPath": "${env:PNPM_HOME}/pnpm",

  // source - file
  "coc.source.file.trimSameExts": ["ts", "js", "tsx", "jsx"],


  "languageserver": {
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["flow"],
      "initializationOptions": {},
      "requireRootPattern": true,
      /* "trace.server": "verbose", */
      "settings": {},
      "rootPatterns": [".flowconfig"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "flux": {
      "command": "flux-lsp",
      "args": ["-l", "/tmp/fluxlsp"],
      "filetypes": ["flux"]
    }
  },

  // extensions: coc-snippets
  "snippets.userSnippetsDirectory": "~/.SpaceVim.d/UltiSnips",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"],
    "flow": ["javascript"]
  },
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.trace": true,
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],

  // extensions: coc-todolist
  "todolist.monitor": true,

  // extensions: coc-explorer
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.root.strategies": ["workspace", "custom:project", "custom:vcs"],
  "explorer.root.customRules": {
    "vcs": {
      "patterns": [".git", ".hg", ".projections.json"]
    },
    "project": {
      "patterns": ["package.json", "go.mod", "Cargo.xml"]
    }
  },

  /* "explorer.floating.width": 400, */
  // "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon] [title] [hidden & 1][root] [fullpath]",
  // "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [link growRight 1 omitCenter 5]",
  "explorer.file.showHiddenFiles": false,
  "explorer.file.hiddenRules": {
    "filenames": [
      ".DS_Store",
      "node_modules",
      ".git",
      "tmp",
      "package-lock",
      "Library"
    ],
    "extensions": [
      "swp",
      "o",
      "a",
      "obj",
      "pyc",
      "lock"
    ],
    "patternMatches": [
    ]
  },
  "explorer.file.reveal.auto": true,
  "explorer.file.reveal.whenOpen": true,
  /* "explorer.floating.border.chars": ["─","│","─","│","┌","┐","┘","└"], */
  // "explorer.openAction.strategy": "vsplit.intelligent",
  "explorer.openAction.strategy": "tab",
  "explorer.keyMappings.global": {
    "i": false, // cancel default mapkey

    "*": "toggleSelection",
    "<tab>": "actionMenu",

    "gk": ["wait", "expandablePrev"],
    "gj": ["wait", "expandableNext"],
    "h": ["wait", "collapse"],
    "l": ["wait", "expandable?", "expand", "open"],
    "J": ["wait", "toggleSelection", "normal:j"],
    "K": ["wait", "toggleSelection", "normal:k"],
    "gl": ["wait", "expand:recursive"],
    "gh": ["wait", "collapse:recursive"],
    "<2-LeftMouse>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    "o": ["wait", "expanded?", "collapse", "expand"],
    "<cr>": ["wait", "expandable?", "cd", "open"],
    "e": "open",
    "s": "open:split",
    "v": "open:vsplit",
    "t": "open:tab",
    "<bs>": ["wait", "gotoParent"],
    "gs": ["wait", "reveal:select"],
    "il": "preview:labeling",
    "ic": "preview:content",
    "Il": "previewOnHover:toggle:labeling",
    "Ic": "previewOnHover:toggle:content",
    "II": "previewOnHover:disable",

    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "p": "pasteFile",
    "df": "delete",
    "dF": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    "g.": "toggleHidden",
    "gc": "toggleOnlyGitChange",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": ["wait", "sourcePrev"],
    "]]": ["wait", "sourceNext"],

    "[i": ["wait", "indentPrev"],
    "]i": ["wait", "indentNext"],
    "u":  ["wait", "indentPrev:insideSource"],

    "[m": ["wait", "markPrev:modified"],
    "]m": ["wait", "markNext:modified"],

    "[d": ["wait", "markPrev:diagnosticError:diagnosticWarning"],
    "]d": ["wait", "markNext:diagnosticError:diagnosticWarning"],
    "[D": ["wait", "markPrev:diagnosticError"],
    "]D": ["wait", "markNext:diagnosticError"],

    "[c": ["wait", "markPrev:git"],
    "]c": ["wait", "markNext:git"],
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
