    {
      "type": "LAN_IP",
      "disabled": true,
      "max_count": 2,
      "show_global": false,
      "ipv6_support": false
    },
    {
      "type": "WAN_IP",
      "disabled": true,
      "ipv4": {
        "dns_query": "myip.opendns.com",
        "dns_resolver": "resolver1.opendns.com",
